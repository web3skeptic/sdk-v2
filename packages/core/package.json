{
  "name": "@circles-sdk/core",
  "version": "0.1.0",
  "description": "Circles Contracts wrapper",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --format esm --minify && tsc --emitDeclarationOnly",
    "dev": "tsc --watch"
  },
  "dependencies": {
    "@circles-sdk/abis": "workspace:*",
    "@circles-sdk/types": "workspace:*",
    "abitype": "^1.1.1",
    "viem": "^2.38.0"
  },
  "devDependencies": {
    "@esbuild-kit/esm-loader": "^2.6.5",
    "@types/bun": "^1.2.23",
    "esbuild": "^0.25.10",
    "typescript": "^5.0.4"
  }
}
