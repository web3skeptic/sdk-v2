{
  "name": "@circles-sdk/rpc",
  "version": "0.1.0",
  "description": "Circles RPC wrapper",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target node --format esm --splitting --external viem --external @circles-sdk/types --external @circles-sdk/utils && tsc --emitDeclarationOnly",
    "dev": "tsc --watch"
  },
  "dependencies": {
    "@circles-sdk/types": "workspace:*",
    "@circles-sdk/utils": "workspace:*",
    "viem": "^2.38.0"
  },
  "devDependencies": {
    "@esbuild-kit/esm-loader": "^2.6.5",
    "@types/bun": "^1.2.23",
    "esbuild": "^0.25.10",
    "typescript": "^5.0.4"
  }
}
