{
  "name": "@circles-sdk-v2/pathfinder",
  "version": "0.1.0",
  "description": "Pathfinding utilities for Circles SDK",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist && tsc --emitDeclarationOnly",
    "dev": "tsc --build --watch",
    "clean": "rm -rf dist tsconfig.tsbuildinfo"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "circles",
    "pathfinding"
  ],
  "license": "MIT",
  "dependencies": {
    "@circles-sdk-v2/rpc": "*",
    "@circles-sdk-v2/types": "*",
    "@circles-sdk-v2/utils": "*",
    "viem": "^2.38.0"
  },
  "devDependencies": {
    "typescript": "^5.0.4"
  }
}
