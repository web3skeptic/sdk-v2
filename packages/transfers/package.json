{
  "name": "@circles-sdk/transfers",
  "version": "0.1.0",
  "description": "Transfer data construction for Circles SDK",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --format esm --splitting && tsc --emitDeclarationOnly",
    "dev": "tsc --build --watch",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "test": "bun test"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "circles",
    "transfers",
    "pathfinding",
    "web3"
  ],
  "license": "MIT",
  "dependencies": {
    "@circles-sdk/types": "workspace:*",
    "@circles-sdk/core": "workspace:*",
    "@circles-sdk/rpc": "workspace:*",
    "@circles-sdk/utils": "workspace:*",
    "@circles-sdk/pathfinder": "workspace:*",
    "viem": "^2.38.0"
  },
  "devDependencies": {
    "typescript": "^5.0.4"
  }
}